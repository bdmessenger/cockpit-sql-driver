<?xml version="1.0" encoding="UTF-8" ?>
<!--
    SQL PostgreSQL Driver
-->
<phpunit
    bootstrap="../bootstrap.php"
    colors="true"
    cacheResult="false"
>
    <php>
        <var name="db_server" value="sqldriver" />
        <var name="db_options_connection" value="pgsql" />
        <var name="db_options_dbname" value="cockpitdb" />
        <var name="db_options_username" value="postgres" />
        <var name="db_options_password" value="" />
    </php>
    <testsuites>
        <testsuite name="MongoHybrid Integration">
            <directory>../lib</directory>
        </testsuite>
    </testsuites>
</phpunit>
